# 测试环境配置
NODE_ENV=test

# 应用配置
VITE_APP_TITLE=Agent Pages (Test)
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=智能代理管理系统 - 测试环境

# API 配置
VITE_API_BASE_URL=https://test-api.agentpages.com/api
VITE_API_TIMEOUT=5000

# 功能开关
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true
VITE_ENABLE_NOTIFICATIONS=false
VITE_ENABLE_HOT_RELOAD=false

# 代理配置
VITE_MAX_AGENTS=3
VITE_AUTO_START_AGENTS=false

# 主题配置
VITE_THEME=light
VITE_PRIMARY_COLOR=#3B82F6

# 测试配置
VITE_ENABLE_MOCK=true
VITE_ENABLE_TEST_LOGGING=true 